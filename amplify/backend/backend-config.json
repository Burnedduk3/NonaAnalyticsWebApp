{
  "function": {
    "lifeprojectauthCustomMessage": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "auth": {
    "userPoolGroups": {
      "service": "Cognito-UserPool-Groups",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "lifeprojectauth",
          "attributes": [
            "UserPoolId",
            "AppClientIDWeb",
            "AppClientID"
          ]
        }
      ]
    },
    "lifeprojectauth": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "lifeprojectauthCustomMessage",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
      ],
      "customAuth": false
    }
  },
  "api": {
    "lifeprojectfinalapp": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "AMAZON_COGNITO_USER_POOLS",
            "userPoolConfig": {
              "userPoolId": "authlifeprojectauth"
            }
          },
          "additionalAuthenticationProviders": [
            {
              "authenticationType": "API_KEY",
              "apiKeyConfig": {
                "apiKeyExpirationDays": 365,
                "description": ""
              }
            },
            {
              "authenticationType": "AWS_IAM"
            }
          ]
        }
      },
      "rdsInit": true
    }
  },
  "storage": {
    "lifeprojectlast": {
      "service": "S3",
      "providerPlugin": "awscloudformation"
    }
  }
}